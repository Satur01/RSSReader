<Application x:Class="RSSReader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:converters="using:RSSReader.Views.Converters"
             xmlns:vmBase="using:RSSReader.ViewModels.ViewModelBase">
    <Application.Resources>
        <ResourceDictionary>
            <converters:GMTToSpanishFormatConverter x:Key="GTMToSpanishFormatConverter" />
            <vmBase:VMLocator x:Key="VmLocator" />
        </ResourceDictionary>
    </Application.Resources>
</Application>